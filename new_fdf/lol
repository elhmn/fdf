	int		ret;
	char	*line;
	int		i;

	fd = 0;
	i = 1;
	if (ac == 2)
		if (!(fd = open(av[1], O_RDONLY)) == -1)
				perror("fd");
	if (ac > 2)
		return (-1);
	while ((ret = get_next_line(fd, &line)))
	{
		if (ret == -1)
